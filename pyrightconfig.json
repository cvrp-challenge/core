{
    "venvPath": ".",
    "venv": ".venv",
    "executionEnvironments": [
        {
            "root": "src",
            "pythonVersion": "3.10",
            "extraPaths": [
                "src",
                "."
            ]
        }
    ],
    "include": [
        "src"
    ],
    "exclude": [
        "**/__pycache__",
        "**/node_modules",
        ".venv",
        "solver"
    ],
    "pythonVersion": "3.10",
    "typeCheckingMode": "off",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false
}

